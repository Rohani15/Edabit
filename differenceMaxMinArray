
public class differenceMaxMin
{

	public static void main(String[] args) 
	{
		System.out.println("Hi");
		
		int A[] = {10, 4, 1, 4, -10, -50, 32, 21};
		
		int min = A[0];
		int max = A[0];
		
		for (int i = 1 ; i < A.length ; i++) 
		{
				if (A[i] > min)
				{
					min = A[i];
				} 
				else if (A[i] < max) 
				{
					max = A[i];
				}
		} 
		return min-max;
	}
}
